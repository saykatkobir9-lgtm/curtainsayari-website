#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/curtainsayari-website/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_b3f380e90f82ece00d4116a10b653b2e/node_modules/vite/bin/node_modules:/mnt/f/curtainsayari-website/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_b3f380e90f82ece00d4116a10b653b2e/node_modules/vite/node_modules:/mnt/f/curtainsayari-website/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_b3f380e90f82ece00d4116a10b653b2e/node_modules:/mnt/f/curtainsayari-website/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/curtainsayari-website/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_b3f380e90f82ece00d4116a10b653b2e/node_modules/vite/bin/node_modules:/mnt/f/curtainsayari-website/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_b3f380e90f82ece00d4116a10b653b2e/node_modules/vite/node_modules:/mnt/f/curtainsayari-website/node_modules/.pnpm/vite@7.2.4_@types+node@24.1_b3f380e90f82ece00d4116a10b653b2e/node_modules:/mnt/f/curtainsayari-website/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vite.js" "$@"
else
  exec node  "$basedir/../../bin/vite.js" "$@"
fi
